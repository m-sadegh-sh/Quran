[
    {
        "id": 1,
        "chapterId": 1,
        "translatorId": 1,
        "text": "باز کردن"
    },
    {
        "id": 2,
        "chapterId": 2,
        "translatorId": 1,
        "text": "گاو ماده"
    },
    {
        "id": 3,
        "chapterId": 3,
        "translatorId": 1,
        "text": "خاندان عمران"
    },
    {
        "id": 4,
        "chapterId": 4,
        "translatorId": 1,
        "text": "زنان"
    },
    {
        "id": 5,
        "chapterId": 5,
        "translatorId": 1,
        "text": "طَبَق غذا (سفره غذا)"
    },
    {
        "id": 6,
        "chapterId": 6,
        "translatorId": 1,
        "text": "چهارپایان"
    },
    {
        "id": 7,
        "chapterId": 7,
        "translatorId": 1,
        "text": "مکانی بین بهشت و جهنم"
    },
    {
        "id": 8,
        "chapterId": 8,
        "translatorId": 1,
        "text": "زیادی، غنیمت، هبه"
    },
    {
        "id": 9,
        "chapterId": 9,
        "translatorId": 1,
        "text": "بازگشتن"
    },
    {
        "id": 10,
        "chapterId": 10,
        "translatorId": 1,
        "text": "نام حضرت یونس (علیه السّلام) یکی از پیامبران خدا"
    },
    {
        "id": 11,
        "chapterId": 11,
        "translatorId": 1,
        "text": "نام حضرت هود (علیه السّلام) یکی از پیامبران خدا"
    },
    {
        "id": 12,
        "chapterId": 12,
        "translatorId": 1,
        "text": "نام حضرت یوسف (علیه السّلام) یکی از پیامبران خدا"
    },
    {
        "id": 13,
        "chapterId": 13,
        "translatorId": 1,
        "text": "صدای غرش ابر"
    },
    {
        "id": 14,
        "chapterId": 14,
        "translatorId": 1,
        "text": "نام حضرت ابراهیم (علیه السّلام) یکی از پیامبران خدا"
    },
    {
        "id": 15,
        "chapterId": 15,
        "translatorId": 1,
        "text": "منع، نام شهر اصحاب الحجر"
    },
    {
        "id": 16,
        "chapterId": 16,
        "translatorId": 1,
        "text": "زنبور عسل"
    },
    {
        "id": 17,
        "chapterId": 17,
        "translatorId": 1,
        "text": "رفتن در شب"
    },
    {
        "id": 18,
        "chapterId": 18,
        "translatorId": 1,
        "text": "غار وسیع"
    },
    {
        "id": 19,
        "chapterId": 19,
        "translatorId": 1,
        "text": "نام حضرت مریم (سلام الله علیها)"
    },
    {
        "id": 20,
        "chapterId": 20,
        "translatorId": 1,
        "text": "از حروف مقطعه و نیز از اسامی پیامبر (صلّی الله علیه و آله)"
    },
    {
        "id": 21,
        "chapterId": 21,
        "translatorId": 1,
        "text": "پیامبران"
    },
    {
        "id": 22,
        "chapterId": 22,
        "translatorId": 1,
        "text": "قصد، نام یکی از فروعات دین"
    },
    {
        "id": 23,
        "chapterId": 23,
        "translatorId": 1,
        "text": "ایمان آورندگان، گروندگان"
    },
    {
        "id": 24,
        "chapterId": 24,
        "translatorId": 1,
        "text": "روشنایی"
    },
    {
        "id": 25,
        "chapterId": 25,
        "translatorId": 1,
        "text": "قرآن، روشنگر و جداکننده حق از باطل"
    },
    {
        "id": 26,
        "chapterId": 26,
        "translatorId": 1,
        "text": "شاعران"
    },
    {
        "id": 27,
        "chapterId": 27,
        "translatorId": 1,
        "text": "مورچه"
    },
    {
        "id": 28,
        "chapterId": 28,
        "translatorId": 1,
        "text": "داستان ها"
    },
    {
        "id": 29,
        "chapterId": 29,
        "translatorId": 1,
        "text": "نام حشره"
    },
    {
        "id": 30,
        "chapterId": 30,
        "translatorId": 1,
        "text": "نام شهر یا کشور"
    },
    {
        "id": 31,
        "chapterId": 31,
        "translatorId": 1,
        "text": "نام فردی حکیم"
    },
    {
        "id": 32,
        "chapterId": 32,
        "translatorId": 1,
        "text": "خضوع و اظهار فروتنی"
    },
    {
        "id": 33,
        "chapterId": 33,
        "translatorId": 1,
        "text": "گروه ها"
    },
    {
        "id": 34,
        "chapterId": 34,
        "translatorId": 1,
        "text": "نام قوم که پادشاه شان زن بود"
    },
    {
        "id": 35,
        "chapterId": 35,
        "translatorId": 1,
        "text": "شکافنده"
    },
    {
        "id": 36,
        "chapterId": 36,
        "translatorId": 1,
        "text": "حروف مقطعه و رمز است"
    },
    {
        "id": 37,
        "chapterId": 37,
        "translatorId": 1,
        "text": "منظم ساختن صفوف"
    },
    {
        "id": 38,
        "chapterId": 38,
        "translatorId": 1,
        "text": "حروف مقطعه قرآن"
    },
    {
        "id": 39,
        "chapterId": 39,
        "translatorId": 1,
        "text": "دسته، جماعت"
    },
    {
        "id": 40,
        "chapterId": 40,
        "translatorId": 1,
        "text": "بخشاینده"
    },
    {
        "id": 41,
        "chapterId": 41,
        "translatorId": 1,
        "text": "بیان روشن"
    },
    {
        "id": 42,
        "chapterId": 42,
        "translatorId": 1,
        "text": "مشورت"
    },
    {
        "id": 43,
        "chapterId": 43,
        "translatorId": 1,
        "text": "زینت"
    },
    {
        "id": 44,
        "chapterId": 44,
        "translatorId": 1,
        "text": "دود"
    },
    {
        "id": 45,
        "chapterId": 45,
        "translatorId": 1,
        "text": "به زانو درآمده"
    },
    {
        "id": 46,
        "chapterId": 46,
        "translatorId": 1,
        "text": "نام مکانی در عمان"
    },
    {
        "id": 47,
        "chapterId": 47,
        "translatorId": 1,
        "text": "نام پیامبر اسلام (صلّی الله علیه و آله)"
    },
    {
        "id": 48,
        "chapterId": 48,
        "translatorId": 1,
        "text": "پیروزی"
    },
    {
        "id": 49,
        "chapterId": 49,
        "translatorId": 1,
        "text": "خانه ها"
    },
    {
        "id": 50,
        "chapterId": 50,
        "translatorId": 1,
        "text": "حروف مقطعه"
    },
    {
        "id": 51,
        "chapterId": 51,
        "translatorId": 1,
        "text": "بادهایی که اشیاء را به پرواز در می آورد"
    },
    {
        "id": 52,
        "chapterId": 52,
        "translatorId": 1,
        "text": "نام کوه است"
    },
    {
        "id": 53,
        "chapterId": 53,
        "translatorId": 1,
        "text": "ستاره"
    },
    {
        "id": 54,
        "chapterId": 54,
        "translatorId": 1,
        "text": "ماه"
    },
    {
        "id": 55,
        "chapterId": 55,
        "translatorId": 1,
        "text": "بخشنده"
    },
    {
        "id": 56,
        "chapterId": 56,
        "translatorId": 1,
        "text": "حادثه عظیم قیامت"
    },
    {
        "id": 57,
        "chapterId": 57,
        "translatorId": 1,
        "text": "آهن"
    },
    {
        "id": 58,
        "chapterId": 58,
        "translatorId": 1,
        "text": "تابیدن طناب، جدل و مناظره"
    },
    {
        "id": 59,
        "chapterId": 59,
        "translatorId": 1,
        "text": "جمع کردن"
    },
    {
        "id": 60,
        "chapterId": 60,
        "translatorId": 1,
        "text": "آزمون شده (مؤنث)"
    },
    {
        "id": 61,
        "chapterId": 61,
        "translatorId": 1,
        "text": "صف کشیدن"
    },
    {
        "id": 62,
        "chapterId": 62,
        "translatorId": 1,
        "text": "آخرین روز هفته"
    },
    {
        "id": 63,
        "chapterId": 63,
        "translatorId": 1,
        "text": "منافقان (نفاق پیشه گان و دورویان)"
    },
    {
        "id": 64,
        "chapterId": 64,
        "translatorId": 1,
        "text": "مغبون کردن یکدیگر"
    },
    {
        "id": 65,
        "chapterId": 65,
        "translatorId": 1,
        "text": "باز کردن عقد نکاح"
    },
    {
        "id": 66,
        "chapterId": 66,
        "translatorId": 1,
        "text": "حرام کردن"
    },
    {
        "id": 67,
        "chapterId": 67,
        "translatorId": 1,
        "text": "حکومت و اداره امور"
    },
    {
        "id": 68,
        "chapterId": 68,
        "translatorId": 1,
        "text": "وسیله نگارش و نوشتن"
    },
    {
        "id": 69,
        "chapterId": 69,
        "translatorId": 1,
        "text": "روز رستاخیز"
    },
    {
        "id": 70,
        "chapterId": 70,
        "translatorId": 1,
        "text": "صعود از پله"
    },
    {
        "id": 71,
        "chapterId": 71,
        "translatorId": 1,
        "text": "نام حضرت نوح (علیه السّلام) یکی از پیامبران خدا"
    },
    {
        "id": 72,
        "chapterId": 72,
        "translatorId": 1,
        "text": "پوشیده، نام موجود نامرئی"
    },
    {
        "id": 73,
        "chapterId": 73,
        "translatorId": 1,
        "text": "پیچیدن پارچه (خطاب به پیامبر (صلّی الله علیه و آله) است که بعد از فرود از غار حرا خود را در پارچه پیچیده بود)"
    },
    {
        "id": 74,
        "chapterId": 74,
        "translatorId": 1,
        "text": "آرامیدن در بستر"
    },
    {
        "id": 75,
        "chapterId": 75,
        "translatorId": 1,
        "text": "به پاداشتن"
    },
    {
        "id": 76,
        "chapterId": 76,
        "translatorId": 1,
        "text": "نوع انسان، بشر"
    },
    {
        "id": 77,
        "chapterId": 77,
        "translatorId": 1,
        "text": "فرستاده شدگان"
    },
    {
        "id": 78,
        "chapterId": 78,
        "translatorId": 1,
        "text": "خبر مهم"
    },
    {
        "id": 79,
        "chapterId": 79,
        "translatorId": 1,
        "text": "کشیدن"
    },
    {
        "id": 80,
        "chapterId": 80,
        "translatorId": 1,
        "text": "عبوس، چهره در هم کشیدن"
    },
    {
        "id": 81,
        "chapterId": 81,
        "translatorId": 1,
        "text": "پیچیدن نور خورشید"
    },
    {
        "id": 82,
        "chapterId": 82,
        "translatorId": 1,
        "text": "شکافته شدن"
    },
    {
        "id": 83,
        "chapterId": 83,
        "translatorId": 1,
        "text": "کم فروشان"
    },
    {
        "id": 84,
        "chapterId": 84,
        "translatorId": 1,
        "text": "شکافته شدن"
    },
    {
        "id": 85,
        "chapterId": 85,
        "translatorId": 1,
        "text": "شیء ظاهر و آشکار"
    },
    {
        "id": 86,
        "chapterId": 86,
        "translatorId": 1,
        "text": "کوبیدن"
    },
    {
        "id": 87,
        "chapterId": 87,
        "translatorId": 1,
        "text": "برتر، بالاتر"
    },
    {
        "id": 88,
        "chapterId": 88,
        "translatorId": 1,
        "text": "پوشاندن"
    },
    {
        "id": 89,
        "chapterId": 89,
        "translatorId": 1,
        "text": "شکافتن وسیع، صبح"
    },
    {
        "id": 90,
        "chapterId": 90,
        "translatorId": 1,
        "text": "شهر (مکه)"
    },
    {
        "id": 91,
        "chapterId": 91,
        "translatorId": 1,
        "text": "خورشید"
    },
    {
        "id": 92,
        "chapterId": 92,
        "translatorId": 1,
        "text": "شب"
    },
    {
        "id": 93,
        "chapterId": 93,
        "translatorId": 1,
        "text": "اوائل روز"
    },
    {
        "id": 94,
        "chapterId": 94,
        "translatorId": 1,
        "text": "شرح صدر، آرامش"
    },
    {
        "id": 95,
        "chapterId": 95,
        "translatorId": 1,
        "text": "انجیر"
    },
    {
        "id": 96,
        "chapterId": 96,
        "translatorId": 1,
        "text": "خون بسته"
    },
    {
        "id": 97,
        "chapterId": 97,
        "translatorId": 1,
        "text": "اندازه گیری و تقدیر"
    },
    {
        "id": 98,
        "chapterId": 98,
        "translatorId": 1,
        "text": "دلیل روشن"
    },
    {
        "id": 99,
        "chapterId": 99,
        "translatorId": 1,
        "text": "به لرزه درآمدن"
    },
    {
        "id": 100,
        "chapterId": 100,
        "translatorId": 1,
        "text": "دویدن با سرعت"
    },
    {
        "id": 101,
        "chapterId": 101,
        "translatorId": 1,
        "text": "حادثه مهم و سخت"
    },
    {
        "id": 102,
        "chapterId": 102,
        "translatorId": 1,
        "text": "تفاخر و فخرفروشی"
    },
    {
        "id": 103,
        "chapterId": 103,
        "translatorId": 1,
        "text": "فشردن، وقت عصر"
    },
    {
        "id": 104,
        "chapterId": 104,
        "translatorId": 1,
        "text": "عیب جو و غیبت کننده"
    },
    {
        "id": 105,
        "chapterId": 105,
        "translatorId": 1,
        "text": "نام حیوان"
    },
    {
        "id": 106,
        "chapterId": 106,
        "translatorId": 1,
        "text": "قدرت، اکتساب، اجتماع. نام قبیله معروف در مکه که پیامبر اسلام (صلّی الله علیه و آله) نیز جزء آن قبیله بود"
    },
    {
        "id": 107,
        "chapterId": 107,
        "translatorId": 1,
        "text": "چیز کم"
    },
    {
        "id": 108,
        "chapterId": 108,
        "translatorId": 1,
        "text": "خیر کثیر و فراوان"
    },
    {
        "id": 109,
        "chapterId": 109,
        "translatorId": 1,
        "text": "کسانی که حق را می پوشانند"
    },
    {
        "id": 110,
        "chapterId": 110,
        "translatorId": 1,
        "text": "پیروزی"
    },
    {
        "id": 111,
        "chapterId": 111,
        "translatorId": 1,
        "text": "طنابی که از الیاف بافته شده"
    },
    {
        "id": 112,
        "chapterId": 112,
        "translatorId": 1,
        "text": "یگانگی"
    },
    {
        "id": 113,
        "chapterId": 113,
        "translatorId": 1,
        "text": "شکافتن، طلوع صبح"
    },
    {
        "id": 114,
        "chapterId": 114,
        "translatorId": 1,
        "text": "مردم"
    }
]